# 2022-10-15 bashrc for git bash alias
# git repo : https://github.com/younguk-ko/vimrc.git
# git bash에서 hard link. 
# ln $HOME/vimfiles/bashrc $HOME/bashrc
alias ,p='vi ~/.bashrc'
alias sp='source ~/.bashrc'
alias vi='/c/Program\ Files/Vim/vim90/gvim.exe &'
alias ll='ls -al'
alias ,v='/c/Program\ Files/Vim/vim90/gvim.exe $HOME/.bashrc &'
